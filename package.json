    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "@testing-library/react-native": "^12.8.1",
    "patch-package": "^8.0.0",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-paper": "^5.12.5",
    "react-native-safe-area-context": "^5.0.0",
    "react-native-screens": "^4.4.0",
    "react-native-vector-icons": "^10.2.0",
    "react-redux": "^9.2.0",
